<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gargc.smartindiahackthon">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.LoginActivity" />
        <activity android:name=".Activity.Startup.SignUpActivity" />
        <activity android:name=".Activity.ResetPasswordActivity" />
        <activity android:name=".Activity.FirstActivity" />
        <activity android:name=".Activity.Startup.StartupFirstActivity" />
        <activity android:name=".Activity.Investor.InvestorFirstActivity" />
        <activity android:name=".Activity.Investor.SignUp.Accreditation" />
        <activity android:name=".Activity.Startup.CreateStartup" />
        <activity android:name=".Activity.Startup.AddFeed" />
        <activity android:name=".Activity.Investor.InvestActivity"></activity>
    </application>

</manifest>